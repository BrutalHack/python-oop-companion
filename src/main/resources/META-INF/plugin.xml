<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <id>io.intenics.python.fileNameMismatch</id>
    <name>class_name.py</name>
    <vendor>Intenics</vendor>
    <idea-version since-build="221"/>

    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.python</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <localInspection language="Python"
                         implementationClass="io.intenics.python.fileNameMismatch.inspections.FileNameMismatchInspection"
                         displayName="Python file name does not match class name" groupName="Python"
                         enabledByDefault="true" level="ERROR"/>
        <annotator language="Python"
                   implementationClass="io.intenics.python.fileNameMismatch.annotator.FileNameMismatchAnnotator"
                   id="Python file name does not match class name"/>
    </extensions>
</idea-plugin>
